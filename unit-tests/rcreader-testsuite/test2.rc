# Test DODS client configuation file.
USE_CACHE=0
MAX_CACHE_SIZE=20
MAX_CACHED_OBJ=5
IGNORE_EXPIRES=0
CACHE_ROOT=/home/jimg/.dods_cache/
DEFAULT_EXPIRES=86400
